<h1>Listing events</h1>

<table cellpadding="4">
  <tr>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><b><%= event.name %></b></td>
    <td><i><%= event.dateline %></i></td>
    <td><%= link_to 'Show', event %> | <%= link_to 'Edit', edit_event_path(event) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New event', new_event_path %>
